{
sections:[
	
      {
         name:CPU,
         elements:[
         {
               STitleBar:{
                  title:"Frequency Settings",
               }
            },
          {
               SSeekBar:{
                  title:"CPU max frequency",
                  unit:"MHz",
                  step:100000,
                  weight:0.001,
                  min:299000,
                  max:1495000,
                  default:1495000,
                  action:"cpuset max"
               }
            },
 	    {
               SSeekBar:{
                  title:"CPU min frequency",
                  unit:"MHz",
                  step:100000,
                  weight:0.001,
                  min:299000,
                  max:1495000,
                  default:299000,
                  action:"cpuset min"
			}
        },
			{
				SOptionList:{
			                  title:"CPU Governor",
			                  description:"Try not to change to 'interactive' upon changing from it as this could cause a timer related kernel panic. I'll try and investigate the bug.",
			                  default:"interactive_pro",
			                  action:"cpuset gov",
			                  values:[
						"interactive",
						"interactive_pro",
						"performance",
						"ondemand",
						"userspace",
						"smartmax_eps",
						"smartmax",
						"conservative",
						"powersave",
						"wave",
			                  ],
	    notify:[
			        {
			            on:APPLY,
			            do:[ REFRESH, CANCEL ],
			            to:"/sys/devices/system/cpu/cpufreq"
			        },
			        {
			            on:REFRESH,
			            do:REFRESH,
			            to:"/sys/devices/system/cpu/cpufreq"
			        }
			    ]
               }
            },
            {
				SLiveLabel:{
					title:"Current CPU Frequency",
					refresh:1000,
					action:"cpufreq"
				}
			},
			{
				SLiveLabel:{
					title:"Exynos Thermal Zone 0",
					refresh:500,
					action:"thermal"
				}
			},
			{
               STitleBar:{
                  title:"Tunables",
               }
            },
			{
			STreeDescriptor:{
			    path:"/sys/devices/system/cpu/cpufreq",
			    generic: {
			        directory: {
			            STreeDescriptor:{
			                generic: {
			                    directory: {},
			                    element: {
			                        SGeneric: { title:"@NAME" }
			                    }
			                },
			                exclude: [
								"freq_table",
								"volt_table",
								"max_freq",
								"min_freq",
								"boostpulse",
								"sampling_rate_current",
								"profile"
			                ]
			            }
			        }
			    },
			}
			},
         ]
